{
  "name": "yt-dlp-wrapper",
  "version": "1.2.2",
  "description": "A simple wrapper for the yt-dlp command line utility.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "esbuild --bundle src/index.ts --format=esm --minify --platform=node --outdir=./dist && tsc --emitDeclarationOnly",
    "dev": "tsc -w",
    "lint": "eslint -c .eslintrc.json src/**/*",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "npm run build",
    "postinstall": "node -e \"if (require('fs').existsSync('.git')) { process.exit(1) } \" || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krunder/yt-dlp-wrapper.git"
  },
  "keywords": [
    "yt-dlp",
    "youtube",
    "youtube-downloader",
    "downloader",
    "youtube-dl",
    "youtube-dlc",
    "youtube-dlp",
    "youtube-dl-wrapper",
    "youtube-dlc-wrapper",
    "youtube-dlp-wrapper"
  ],
  "author": "Kyle Underwood",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krunder/yt-dlp-wrapper/issues"
  },
  "homepage": "https://github.com/krunder/yt-dlp-wrapper#readme",
  "files": [
    "dist/**/*",
    "bin/**/*"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "~6.0.2",
    "@semantic-release/git": "~10.0.1",
    "@types/bytes": "^3.1.1",
    "@types/jest": "~29.4.0",
    "@types/node": "~18.14.2",
    "@typescript-eslint/eslint-plugin": "~5.53.0",
    "@typescript-eslint/parser": "~5.53.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "husky": "~8.0.3",
    "jest": "~29.5.0",
    "semantic-release": "~20.1.1",
    "ts-jest": "~29.0.5",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "bytes": "^3.1.2",
    "child_process": "^1.0.2",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.17.19",
    "events": "^3.3.0",
    "os": "^0.1.2",
    "p-queue": "^7.3.4",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "tree-kill": "^1.2.2"
  }
}
